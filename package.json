{
  "name": "codeandstuff-discord-bot",
  "version": "1.1.1-beta1",
  "description": "A discord bot for the Codeandstuff Discord server",
  "main": "src/app.js",
  "scripts": {
    "clean": "rm -rf src/js/",
    "compile": "tsc --version && tsc --project tsconfig.json",
    "quality:eslint": "eslint . --ext .ts,.js",
    "quality:eslint:fix": "eslint . --fix",
    "start": "node out/app.js",
    "test": "npm run test:js && npm run test:ts",
    "test:js": "nyc --all mocha --config test/.js.mocharc.js",
    "test:ts": "TS_NODE_PROJECT=./tsconfig.json nyc mocha --config test/.ts.mocharc.js src/**/*.ts",
    "test:coverage": "nyc check-coverage"
  },
  "keywords": [
    "discord",
    "codeandstuff",
    "bot"
  ],
  "author": "Jayne Doe <j4numbers@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@j4numbers/eslint-typescript-config": "^1.0.5",
    "axios": "^0.21.1",
    "bunyan": "^1.8.15",
    "config": "^3.3.6",
    "discord.js": "^12.5.3",
    "eventbrite": "^1.3.0",
    "luxon": "^1.27.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@j4numbers/eslint-base-config": "^1.2.4",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.18",
    "@types/config": "^0.0.38",
    "@types/luxon": "^1.27.0",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "clear-module": "^4.1.1",
    "eslint": "^7.29.0",
    "import-fresh": "^3.3.0",
    "mocha": "^9.0.1",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0",
    "sinon": "^11.1.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
