{
  "name": "codeandstuff-discord-bot",
  "version": "1.0.0",
  "description": "A discord bot for the Codeandstuff Discord server",
  "main": "src/app.js",
  "scripts": {
    "clean": "rm -rf src/js/",
    "compile": "tsc --project src/tsconfig.json",
    "quality:eslint": "eslint .",
    "quality:eslint:fix": "eslint . --fix",
    "start": "node src/app.js",
    "test": "npm run test:js && npm run test:ts",
    "test:js": "nyc --all mocha --config test/.js.mocharc.js",
    "test:ts": "TS_NODE_PROJECT=./src/tsconfig.json nyc mocha --config test/.ts.mocharc.js src/ts/**/*.ts",
    "test:coverage": "nyc check-coverage"
  },
  "keywords": [
    "discord",
    "codeandstuff",
    "bot"
  ],
  "author": "Jayne Doe <j4numbers@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "bunyan": "^1.8.14",
    "config": "^3.3.1",
    "discord.js": "^12.2.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@j4numbers/eslint-base-config": "^1.1.8",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.12",
    "@types/config": "0.0.36",
    "@types/mocha": "^8.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "clear-module": "^4.1.1",
    "eslint": "^7.6.0",
    "import-fresh": "^3.2.1",
    "mocha": "^8.1.1",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
